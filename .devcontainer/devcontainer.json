{
    "name": "FastAPI Dev",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "api",
    "workspaceFolder": "/app",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-azuretools.vscode-docker"
        ],
        "settings": {
          "python.defaultInterpreterPath": "/usr/local/bin/python",
          "editor.formatOnSave": true
        }
      }
    }
  }
  